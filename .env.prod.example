# Database (RDS)
DB_URL=jdbc:postgresql://tasktracker-db.xxxxx.us-east-2.rds.amazonaws.com:5432/taskdb
DB_USERNAME=taskuser
DB_PASSWORD=your-rds-password

# Google OAuth
GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com

# JWT
JWT_SECRET=replace-with-32+char-secret
JWT_MINUTES=15
JWT_COOKIE_SECURE=true

# Rate limiting
AUTH_RL_WINDOW_SECONDS=60
AUTH_RL_MAX_REQUESTS=10

# CORS (production domain)
CORS_ALLOWED_ORIGINS=https://tasktracker.nicolasgrabner.com
